    <div class="container">

      <nav id="mySidenav" class="sidenav wrapper">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>

        <div class="fresh-items">
          <h4>TOPICS</h4>
          <ul style="list-style: none; padding: 0; margin: 0;">
            <li><a href="#" onclick="logate('topic=arts-and-entertainment'); return false;">Arts and Entertainment</a></li>
            <li><a href="#" onclick="logate('topic=business'); return false;">Business</a></li>
            <li><a href="#" onclick="logate('topic=culture'); return false;">Culture</a></li>
            <li><a href="#" onclick="logate('topic=everyday-life'); return false;">Everyday Life</a></li>
          </ul>
        </div> <!-- .news -->

        <div class="fresh-items">
          <h4>DIFFICULTY</h4>
          <ul style="list-style: none; padding: 0; margin: 0;">
            <li><a href="#" onclick="logate('difficulty=1'); return false;">Beginner</a></li>
            <li><a href="#" onclick="logate('difficulty=2'); return false;">Elementary</a></li>
            <li><a href="#" onclick="logate('difficulty=3'); return false;">Intermediate</a></li>
            <li><a href="#" onclick="logate('difficulty=4'); return false;">Upper Intermediate</a></li>
            <li><a href="#" onclick="logate('difficulty=5'); return false;">Advanced</a></li>
            <li><a href="#" onclick="logate('difficulty=6'); return false;">Native</a></li>
          </ul>
        </div> <!-- .news -->

        <div class="fresh-items">
          <h4>FORMATS</h4>
          <ul style="list-style: none; padding: 0; margin: 0;">
            <li><a href="#" onclick="logate('category=clips'); return false;">Clips</a></li>
            <li><a href="#" onclick="logate('category=commercials'); return false;">Commercials</a></li>
            <li><a href="#" onclick="logate('category=mini-movies'); return false;">Mini-Movies</a></li>
            <li><a href="#" onclick="logate('category=movie-trailers'); return false;">Movie Trailers</a></li>
            <li><a href="#" onclick="logate('category=music-videos'); return false;">Music Videos</a></li>
            <li><a href="#" onclick="logate('category=news'); return false;">News</a></li>
          </ul>
        </div> <!-- .news -->

        <div class="quick-links">
          <h4>Quick Links</h4>
          <ul style="list-style: none; padding: 0; margin: 0;">
            <li><a href="#0">All</a></li>
            <li><a href="#0">Videos</a></li>
            <li><a href="#0">Audios</a></li>
            <li><a href="#0">Playlists</a></li>
          </ul>
        </div> <!-- .quick-links -->

        <script type="text/javascript">
          function logate(param) {
            params = document.location.search.split('?')[1] ? document.location.search.split('?')[1].split('&') : []
            for (i = 0; i < params.length; i++) { 
              if (params[i].split('=')[0] == param.split('=')[0]) { 
                params.splice(i, 1);
                document.location.search = '?' + params.join('&') + '&' + param;
                return
              }
            }
            document.location.search += (document.location.search.split('?')[1] ? '&':'?') + param
          }
        </script>

        <style type="text/css">
          .fresh-items {
            float: left;
          }

          .quick-links {
            margin-top:50px;
            float: right;
            width: 140px;
            margin-right: 15px;
            text-align: right;
          }

          .quick-links a {
            padding: 0 0px 7px 10px;
          }
        </style>
      </nav>
      <script type="text/javascript">
          /* Set the width of the side navigation to 250px */
          function openNav(e) {
              if (e.getAttribute('active') == 'true') {
                closeNav()
              } else {
                document.getElementById("mySidenav").style.height = "210px";
                e.setAttribute('active', true)
              }
          }

          /* Set the width of the side navigation to 0 */
          function closeNav() {
              document.getElementById("mySidenav").style.height = "0";
              document.getElementById("openNavButton").setAttribute("active", false)
          }
      </script>

      <p class="alert alert-info" role="alert"><%= get_flash(@conn, :info) %></p>
      <p class="alert alert-danger" role="alert"><%= get_flash(@conn, :error) %></p>

      <main role="main">
        <%= render @view_module, @view_template, assigns %>
      </main>

      <footer class="bottomfooter" role="contentinfo"></footer>

      <%= #render("_player.html", assigns) %>
    </div> <!-- /container -->
  </body>
