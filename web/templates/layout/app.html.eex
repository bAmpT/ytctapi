  <style type="text/css">

  body.nav-open .navigation-tray_curtain {
    opacity: .8;
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    visibility: visible;
  }

  .navigation-tray_curtain {
    background: #121921;
    cursor: pointer;
    opacity: 0;
    transition: all .2s ease-in-out;
    visibility: hidden;
    will-change: transform opacity;
    z-index: 999;
  }

.navigation-bar {
    display: flex;
    position: relative;
    justify-content: flex-start;
    align-content: center;
    align-items: center;
    padding: .85714rem;
    height: 78px;
    z-index: 1000;
}

.navigation-tray_content {
    display: flex;
    justify-content: space-around;
    padding: 0 .85714rem 1.85714rem;
    background-color: #121921;
    border: 1px solid #61c192;
    border-top: 0;
}

.navigation-tray {
    font-size: .8em;
    overflow: hidden;
}

.navigation-bar_menu-button {
    flex: 0 0 auto;
    min-width: 3rem;
    padding: 1rem;
    margin: auto 0 auto 0;
}

.navigation-bar_menu-button > img {
    width: 1.9rem;
    height: 1.25rem
}

  .navigation-tray-section {
    margin-bottom: 0;
    margin-right: 1rem;
    flex: 1 1 25%;
    max-width: 12.5rem;
  }

  .navigation-tray-section--aside {
    display: none;
    margin-right: 6%;
    max-width: 22%;
  }

.navigation-bar_tray {
    z-index: 1000;
    padding: 0 3.85714rem 3.85714rem;
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
}

.navigation-bar_tray {
    filter: blur(0);
    opacity: 1;
    pointer-events: inherit;
    transform: scale(1) translateZ(0);
    visibility: visible
}

.navigation-bar-dropdown {
    background-color: #121921;
    border-left: 1px solid #61c192
}

.navigation-bar-dropdown > a {
    color: #61c192;
    display: block;
    line-height: 2em;
    padding: 0 1rem
}

.navigation-bar-link {
    flex: 0 0 auto;
    padding: 1rem;
    font-size: 1.3em;
    position: relative
}

.navigation-bar-link, .navigation-bar-link > a {
    margin-left: 1px;
    color: #61c192
}

.navigation-bar-link--is-open, .navigation-bar-link--is-open > a,
.navigation-bar-link:hover, .navigation-bar-link:hover > a {
    color: #fff
}

.navigation-bar-link--is-open {
    margin-left: 0;
    border-left: 1px solid #61c192
}

.navigation-bar-link_dropdown {
    display: none
}

.navigation-bar-link--is-open .navigation-bar-link_dropdown {
    display: block;
    position: absolute;
    top: 99%;
    left: -1px
}


  </style>
